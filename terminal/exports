#################################################
# EXPORT USER SPECIFIC VARIABLES TO THE SHELL   #
#################################################

export CDPATH=$CDPATH:$ROOTDIR
export EDITOR="vim -u $CUSTOM_CONFIG_PACKAGES_DIR/vim/vimrc"
export HISTTIMEFORMAT="%d/%m/%y %T"
export HISTFILESIZE=1000000000
export HISTSIZE=1000000
export SSH_ASKPASS=""
export TERM=xterm-256color
export FIGNORE=".o:"
